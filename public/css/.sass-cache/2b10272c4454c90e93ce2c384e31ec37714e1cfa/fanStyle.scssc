menu.fullMenu{
	@include transition(0.25s);
	border-bottom: solid 1px $secondary;

	ul{
		@include transition(0.25s);
		background: $link-color;
		overflow: hidden;
		li{
			float: left;
			border-left: solid 1px $secondary;
			border-right: solid 1px $secondary;
			background: lighten($link-color, 15%);
			a{
				@include transition(0.25s);
				font-family: 'Lato', sans-serif;
				color: #fff;
				font-weight: 400;
				text-decoration: none;
			}
		}
		li:hover{
			background: lighten($secondary, 15%);
			-moz-box-shadow: inset 0 0 5px 1px $primary;
			-webkit-box-shadow: inset 0 0 5px 1px $primary;
			box-shadow: inset 0 0 5px 1px $primary;
			a{
				color: #fff;
			}
		}
		li.current{
			background: $primary;
			a{
				color: #fff;
				text-shadow: 1px 0px 1px #aaa;
			}
		}
		li.current:hover{
			background: $secondary;
		}
		li:first-child{
			margin-left: 15%;
		}

	}
	ul:hover{
		//background: #fff;
	}
	.logo{
		@include transition(0.25s);
		position: absolute;
		font-family: 'Megrim', cursive;
		text-shadow: 1px 0px 1px #aaa;
		color: #fff;
		top: 0;
		padding: 15px;
		padding-left: 20px;
		padding-right: 20px;
		right: 10%;
		background: opacify($primary, 0.5);
		border-bottom-left-radius: 5px;
		border-bottom-right-radius: 5px;
		display: block;
	}
	.logo:hover{
		background: opacify($secondary, 0.5);
		padding: 20px;
		-moz-box-shadow: 0 0 5px 1px $primary;
		-webkit-box-shadow: 0 0 5px 1px $primary;
		box-shadow: 0 0 5px 1px $primary;
	}

}

