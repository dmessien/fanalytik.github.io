/* HOME PAGE */

.slider{
	width: 100%;
	height: 500px;
	position: relative;
	.logo{
		position: absolute;
		top: 50px;
		right: 5%;
		z-index: 400;
		h1{
			font-family: 'Megrim', cursive;
			font-size: 40px;
			color: #fff;
			text-shadow: 1px 0px 1px #484848;
		}
	}
}

.slide{
	height: 100%; 
	width: 100%;
	font-family: 'Lato', sans-serif; 
	position: absolute; 
	top: 0; 
	-moz-box-shadow: inset 0 -40px 50px 0 #000;
	-webkit-box-shadow: inset 0 -40px 50px 0 #000;
	box-shadow: inset 0 -40px 50px 0 #000;
}
.slideText{
	position: absolute; 
	bottom: 35%; left: 20%; 
	width: 70%; color: white; 
	border-radius: 15px; 
	padding-top: 15px; padding-bottom: 15px;
}
.slideText h1, .slideText h2, .slideText h3{
	margin-left: 50px; 
	color: white;
	text-shadow: 1px 0px 1px #000;
}
.socialBox{
	@include transition(0.25s);
	position: absolute;
	top: 25px;
	left: 5%;
	z-index: 500;
	text-align: center;
	width: 50px;
	.socialLinks{
		width: 40px;
		height: 40px;
		margin-bottom: 25px;
		opacity: 0.90;
		display: block;
	}
	.socialLinks:hover{
		opacity: 1.0;
	}
}
.slideRadioBox{
	@include transition(0.25s);
	position: absolute;
	bottom: 25px;
	z-index: 400;
	width: 100%;
	text-align: center;
	.slideRadio{
		@include transition(0.25s);
		@include circleBorder(10px);
		border: solid 1px #f2f2f2;
		background: #fff;
		margin-right: 15px;
		opacity: 0.90;
		display: inline-block;
	}
	.slideRadio:hover{
		background: lighten($link-color, 25%);
		opacity: 1.0;
	}
	.slideRadio.current{
		background: $link-color;
	}
}
.slideText{
	h1{font-size: 55px;}
	h2{
		font-family: 'Lato', sans-serif;
		font-size: 35px; 
		font-weight: 700;
	} 
	h3{
		font-family: 'Lato', sans-serif;
		font-size: 20px; 
		font-weight: 400;
		width: 600px;
	}  
	hr{
		width: 70%;
		margin-left: 50px;
		margin-top: 10px;
		margin-bottom: 10px;
	}
	a{color: #6d82ff;} 
	a:hover{color: #eee;}
} 
#slide1{background: url(/images/nfl_grass.jpg);}
#slide2{background: url(/images/soccer_inspiration.jpg);}
#slide3{background: url(/images/microphone.jpg);} 
#slide4{background: url(/images/soccer_fans.jpg);}
#slide5{background: url(/images/old_field.jpg);}

.slide.ng-hide-add, .slide.ng-hide-remove {
  -webkit-transition:all linear 0.5s;
  -moz-transition:all linear 0.5s;
  -o-transition:all linear 0.5s;
  transition:all linear 0.5s;
  display:block!important;
}

.slide.ng-hide-add.ng-hide-add-active,
.slide.ng-hide-remove {
  opacity:0;
}

.slide.ng-hide-add,
.slide.ng-hide-remove.ng-hide-remove-active {
  opacity:1;
}

.arrows{position: absolute; top: 80%; opacity: 0.85; z-index: 200;} 
.arrows:hover{opacity: 1.0;} 
.arrows img{height: 50px; width: 50px;}
#leftArrow{left: 5%;} #rightArrow{right: 5%;}

#home2{
	font-family: 'Lato', sans-serif;
	padding-top: 25px;
	padding-bottom: 40px;
	background: $link-color;
	text-align: center;
	color: #fff;
	h2{
		font-weight: 400;
	}
	h3{
		font-weight: 300;
	}
	hr{
		width: 80%;
		margin: 0 auto;
		margin-top: 10px;
		margin-bottom: 10px;
	}
}

#home3{
	font-family: 'Lato', sans-serif;
	background: #fff;
	text-align: center;
	padding-top: 100px;

	h2{
		text-align: center;
		color: $link-color;
	}
	hr{
		width: 80%;
		margin: 0 auto;
		margin-top: 10px;
		margin-bottom: 15px;
	}
	.infoSquare{
		@include transition(0.25s);
		width: 40%;
		margin: 2%;
		padding-top: 25px;
		padding-bottom: 35px;
		background: #eee;
		border-radius: 10px;
		display: inline-block;
		-moz-box-shadow: 0 0 5px 1px #aaa;
		-webkit-box-shadow: 0 0 5px 1px #aaa;
		box-shadow: 0 0 5px 1px #aaa;

		h2, h3{
			text-align: center;
		}
		p{
			font-weight: 300;
			margin-top: 10px;
			margin-left: 5%;
			margin-right: 5%;
		}
		img{
			margin-top: 15px;
			margin-bottom: 15px;
			width: 80%;
			height: 300px;
			border: solid 1px #aaa;
			-moz-box-shadow: 0 0 5px 1px #484848;
			-webkit-box-shadow: 0 0 5px 1px #484848;
			box-shadow: 0 0 5px 1px #484848;

		}
	}
	.infoSquare:hover{
		background: lighten(#f2f2f2, 15%);
		-moz-box-shadow: 0 0 5px 1px #484848;
		-webkit-box-shadow: 0 0 5px 1px #484848;
		box-shadow: 0 0 5px 1px #484848;
		h2{
			color: $primary;
		}
		img{
			-moz-box-shadow: 0 0 5px 1px #000;
			-webkit-box-shadow: 0 0 5px 1px #000;
			box-shadow: 0 0 5px 1px #000;

		}
	}

}



