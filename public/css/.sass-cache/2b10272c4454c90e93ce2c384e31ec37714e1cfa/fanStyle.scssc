:@template"é1$primary: #5CE65C;
$secondary: #66FF99;
$link-color: #3CA0D0;
$font-color1: #484848;
$font-color2: lighten(#484848, 40%);

@mixin transition($time){
    -webkit-transition: all linear $time;
	-moz-transition: all linear $time;
	-o-transition: all linear $time;
	transition: all linear $time;
}

@mixin basicBorder($color){
	border: solid 1px $color;
}

@mixin circleBorder($diameter){
	height: $diameter;
	width: $diameter;
	-webkit-border-radius: 50%;
	 -moz-border-radius: 50%;
	  -ms-border-radius: 50%;
	   -o-border-radius: 50%;
	      border-radius: 50%;
}

@mixin border-radius($radius) {
  -webkit-border-radius: $radius;
     -moz-border-radius: $radius;
      -ms-border-radius: $radius;
          border-radius: $radius;
}

@mixin boxTopShadow($color){
	-moz-box-shadow: inset 0 7px 30px -4px $color;
	-webkit-box-shadow: inset 0 7px 30px -4px $color;
	box-shadow: inset 0 7px 30px -4px $color;
}

@mixin simpleBoxShadow($color){
	-moz-box-shadow: 0px 0px 10px 1px $color;
	-webkit-box-shadow: 0px 0px 10px 1px $color;
	box-shadow: 0px 0px 10px 1px $color;
}

@mixin simpleBoxShadowInset($color){
	-moz-box-shadow: inset 0px 0px 10px 1px $color;
	-webkit-box-shadow: inset 0px 0px 10px 1px $color;
	box-shadow: inset 0px 0px 10px 1px $color;
}

*{
	margin: 0;
	padding: 0;
}

body{
	background-repeat: no-repeat;
	/*-webkit-box-shadow: inset 0px -80px 1000px -10px rgba(255,255,255,0.8);
	-moz-box-shadow: inset 0px -80px 1000px -10px rgba(255,255,255,0.8);
	box-shadow: inset 0px -80px 1000px -10px rgba(255,255,255,0.8);*/
}

.secFull{
	width: 100%;
}

a{
	@include transition(0.25s);
	color: $link-color;
	cursor: pointer;
}

a:hover{
	color: darken($link-color, 15%);
}

.hide{
	display: block;
}

button{
	@include transition(0.25s);
	border: solid 1px darken(#f2f2f2, 25%);
	border-radius: 5px;
	padding: 10px;
	background: #f2f2f2;
}

button:hover{
	background: #fff;
}

.mainBump{
	padding-top: 75px;
}

menu{
	@include transition(0.25s);
	width: 100%;
	position: fixed;
	top: 0;
	z-index: 800;
	//border-bottom: solid 1px $link-color;

	ul{
		@include transition(0.25s);
		//background: url(/images/whiteTrans90.png);
		overflow: hidden;
		li{
			float: left;
			cursor: pointer;
			margin-left: 2.5%;
			padding: 15px;
			padding-left: 25px;
			padding-right: 25px;
			min-width: 100px;
			text-align: center;
			//border-left: solid 1px $secondary;
			//border-right: solid 1px $secondary;
			list-style: none;
			font-size: 20px;
			a{
				@include transition(0.25s);
				font-family: 'Lato', sans-serif;
				//color: $link-color;
				color: #fff;
				font-weight: 400;
				text-decoration: none;
			}
			a:hover{
				color: $link-color;
			}
		}
		li:hover{
			/*background: lighten($secondary, 15%);
			-moz-box-shadow: inset 0 0 5px 1px $primary;
			-webkit-box-shadow: inset 0 0 5px 1px $primary;
			box-shadow: inset 0 0 5px 1px $primary;*/
			border-left: solid 1px #eee;
			border-right: solid 1px #eee;
			border-bottom: solid 1px #eee;
			a{
				color: #fff;
			}
		}
		li.current{
			//background: $primary;
			a{
				//color: $secondary;
